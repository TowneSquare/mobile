{
  "expo": {
    "name": "TowneSquare",
    "slug": "TowneSquare",
    "scheme": "townesquare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/png/appLogo.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.townesquare.townesquare"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fde1a842-16d1-47e7-a064-87974daf80de"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "plugins": [
      [
        "expo-image-picker", 
        {
        "photoPermission": "This app needs permission to access your photos."
        }
      ]
    ]
  }
}
